{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "id": "worker_events",
  "type": "object",
  "properties": {
    "events": {
      "type": "array",
      "items": {
        "type": "object",
        "required": [
          "time",
          "message"
        ],
        "properties": {
          "time": {
            "type": "string",
            "format": "date-time"
          },
          "message": {
            "type": "string"
          }
        }
      }
    }
  }
}
